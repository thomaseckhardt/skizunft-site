{
  "name": "skizunft-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "PUBLIC_ENV=development astro dev --host",
    "develop": "npm run dev",
    "start": "npm run dev",
    "prebuild": "node generate-google-api-credentials.js",
    "build": "PUBLIC_ENV=production astro build",
    "build:preview": "PUBLIC_ENV=preview astro build",
    "preview": "astro preview --host",
    "astro": "astro",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "sb-login": "storyblok login",
    "sb-pull-components": "dotenv -e ./.env.local cross-var storyblok pull-components --space %STORYBLOK_SPACE%",
    "sb-generate-types": "dotenv -e ./.env.local cross-var storyblok-generate-ts source=./components.%STORYBLOK_SPACE%.json target=./src/types/storyblok-component-types.d.ts",
    "sb-graphiql": "dotenv -e ./.env.local cross-var open https://gapi-browser.storyblok.com/?token=%STORYBLOK_TOKEN%",
    "sb": "storyblok",
    "sb:components": "storyblok components pull --space 256004 --verbose --separate-files",
    "sb:types": "storyblok types generate --space 256004 --verbose --separate-files --strict --type-prefix Storyblok",
    "sb:sync": "pnpm run sb:components && pnpm run sb:types",
    "tailwindcss": "npx tailwindcss -i ./src/styles/base.css -o ./tailwind/base.css --watch",
    "func:send-booking-confirmation": "netlify functions:invoke send-booking-confirmation-v2 --payload '{\"firstName\": \"John\", \"lastName\": \"Doe\", \"email\": \"thomas.eckhardt@web.de\", \"orderNumber\": \"123456\"}' --port 9999"
  },
  "dependencies": {
    "@alpinejs/collapse": "^3.14.7",
    "@alpinejs/persist": "^3.14.7",
    "@astrojs/mdx": "^3.1.9",
    "@astrojs/netlify": "^5.5.4",
    "@astrojs/react": "^4.1.0",
    "@astrojs/rss": "^4.0.10",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "^5.1.3",
    "@faker-js/faker": "^9.3.0",
    "@getbrevo/brevo": "^2.2.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@netlify/emails": "^1.1.0",
    "@netlify/functions": "^2.8.2",
    "@storyblok/astro": "^5.0.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/react": "^18.3.25",
    "@types/react-dom": "^18.3.7",
    "@unpic/astro": "^1.0.1",
    "alpinejs": "^3.14.7",
    "astro": "^4.16.17",
    "astro-icon": "^1.1.4",
    "blurhash": "^2.0.5",
    "clsx": "^2.1.1",
    "convex": "^1.17.3",
    "google-spreadsheet": "^5.0.2",
    "googleapis": "^144.0.0",
    "handlebars": "^4.7.8",
    "mjml": "^4.15.3",
    "nodemailer-mjml": "^1.4.7",
    "object-to-formdata": "^4.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.54.1",
    "storyblok-rich-text-astro-renderer": "^2.2.2",
    "tailwindcss": "^3.4.16",
    "unpic": "^4.1.3",
    "uuid": "^10.0.0",
    "vite": "^5.4.11"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.2",
    "@iconify/react": "^5.0.2",
    "@types/node": "^22.10.2",
    "cross-var": "^1.1.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.4",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "storyblok": "^4.6.3",
    "storyblok-js-client": "^6.10.3"
  }
}
